{
  "name": "wuber-server",
  "version": "1.0.0",
  "description": "wuber-server with graphql",
  "main": "index.js",
  "repository": "https://apJammanbo@github.com/apJammanbo/wuber-server.git",
  "author": "apJammanbo <apJammanbo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "graphql-tools": "^3.0.5",
    "graphql-yoga": "^1.14.12",
    "helmet": "^3.13.0",
    "merge-graphql-schemas": "^1.5.3",
    "morgan": "^1.9.0",
    "tslint": "^5.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/helmet": "^0.0.38",
    "@types/morgan": "^1.7.35",
    "babel-runtime": "^6.26.0",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  }
}
